{
  "manifest_version": 3,
  "name": "Read Recap",
  "version": "1.0",
  "description": "Recaps significant articles from your browsing history",
  "permissions": [
    "history",
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
